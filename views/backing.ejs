<body class="bg-white">
    <!-- app capsule  -->

    <!--* app capsule  -->
    <!-- loader -->
    <div id="loader">
        <img src="/assets/img/logo-icon.png" alt="icon" class="loading-icon">
    </div>
    <!-- * loader -->
    <div class="appCasule">
        <!-- header  -->
        <div class="appHeader bg-white mt-1 no-border" style="position: relative; z-index: 0; ">

            <div class="pageTitle">Backing And Investment</div>
            <div class="right">
                <div  class="headerButton">
                    <ion-icon class="round bg-white  text-dark " name="add-"></ion-icon>
                </div>

            </div>
        </div>
        <!-- * header  -->



        <br><br><br>
        <!-- Things  -->
        <div class="section mt-4 ">
            <div class="row mt-2">
                <div class="col-6">
                    <div  class="stat-box  -shadow  bg-primary"style="background-color: RGBA(138,74,243,1);">
                        <a href="/backings">
                            <ion-icon  class="value text-white " size="large" name="rocket-outline"></ion-icon>

                            <div class="h6 text-white total">Backings</div>
                        </a>
                    </div>
                </div>
                <div class="col-6">
                    <div class="stat-box fancy-   bg-success  " style="background-color: RGBA(74,243,138,1)">
                        <a href="/myInvestment">
                            <ion-icon class = 'value text-white' size="large" name="cash-outline"></ion-icon>

                            <div class=" text-white total h6 ">My Investments</div>
                        </a>
                    </div>
                </div> <hr>
                <div class="col-12">
                    <div class="stat-box fancy-   bg-dark  " style="background-color: RGBA(74,243,138,1)">
                        <a href="/all">
                            <ion-icon class = 'value text-white' size="large" name="wallet-outline"></ion-icon>

                            <div class=" text-white total h6 ">Active Investments</div>
                        </a>
                    </div>
                </div>  

            </div>

        </div>
        <!-- * Things  -->

        
    </div>

    <!-- App Bottom Menu -->
    <div class="appBottomMenu ">
        <a href="/home" class="item ">
            <div class="col">
                <ion-icon name="pie-chart-outline"></ion-icon>
                <strong>Overview</strong>
            </div>
        </a>
        <a href="/banking" class="item ">
            <div class="col">
                <ion-icon name="wallet-outline"></ion-icon>
                <strong>Banking</strong>
            </div>
        </a>
        <a href="/backing" class="item active">
            <div class="col">
                <ion-icon name="rocket-outline"></ion-icon>
                <strong>Backing</strong>
            </div>
        </a>

        <a href="/settings" class="item" data-toggle="modal" data-target="#sidebarPanel">
            <div class="col">
                <ion-icon name="settings-outline"></ion-icon>
                <strong>More</strong>
            </div>
        </a>
    </div>
    <!-- * App Bottom Menu -->

  

    <!-- ///////////// Js Files ////////////////////  -->
    <!-- Jquery -->
    <script src="/assets/js/lib/jquery-3.4.1.min.js"></script>
    <!-- Bootstrap-->
    <script src="/assets/js/lib/popper.min.js"></script>
    <script src="/assets/js/lib/bootstrap.min.js"></script>
    <!-- Ionicons -->
    <script src="/assets/js/icons/ionicons.js"></script>    <!-- Owl Carousel -->
    <script src="/assets/js/plugins/owl-carousel/owl.carousel.min.js"></script>
